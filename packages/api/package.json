{
	"name": "@chess-d/api",
	"version": "0.0.0",
	"private": true,
	"license": "MIT",
	"scripts": {
		"dev": "pnpm build --watch",
		"build": "tsc -b -v",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"test": "jest --passWithNoTests"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist/**"
	],
	"publishConfig": {
		"access": "public"
	},
	"typesVersions": {
		"*": {
			"*": [
				"src/*"
			]
		}
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		},
		"./*": {
			"import": "./dist/*.js",
			"require": "./dist/*.js"
		}
	},
	"dependencies": {
		"@nestjs/mapped-types": "*"
	},
	"devDependencies": {
		"@chess-d/configs": "workspace:*",
		"@types/node": "^20.3.1",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.2"
	}
}
