{
	"name": "@chess-d/api",
	"version": "0.1.0",
	"private": true,
	"license": "MIT",
	"scripts": {
		"dev": "pnpm build --watch",
		"build": "tsc -b -v",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"test": "jest --passWithNoTests"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist/**"
	],
	"publishConfig": {
		"access": "public"
	},
	"typesVersions": {
		"*": {
			"*": [
				"src/*"
			]
		}
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		},
		"./*": {
			"import": "./dist/*.js",
			"require": "./dist/*.js"
		}
	},
	"dependencies": {
		"@apollo/server": "^4.10.4",
		"@nestjs/apollo": "^12.2.0",
		"@nestjs/mapped-types": "*",
		"apollo-server-plugin-base": "^3.7.2",
		"chess.js": "1.0.0-beta.8",
		"three": "^0.169.0"
	},
	"devDependencies": {
		"@chess-d/configs": "workspace:*",
		"@types/node": "^22.5.1",
		"@types/three": "^0.169.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3"
	}
}
