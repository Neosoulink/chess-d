{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Server Config",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "ES2021",
		"allowSyntheticDefaultImports": true,
		"incremental": true,

		"module": "Node16",
		"esModuleInterop": true,
		"moduleResolution": "Node16",

		"sourceMap": true,

		"strictNullChecks": false,
		"strictBindCallApply": false
	}
}
